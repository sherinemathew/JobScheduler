<table>
    <tr>
        <th>Job</th>
        <th>Location</th>
        <th>Start Date</th>
        <th>Days to Complete</th>
        <th>Complete?</th>
        <th>Employees Assigned</th>
    </tr>
    <% @jobs.each do |job| %>
        <tr>
            <td><%= job.name %></td>
            <td><%= job.location %></td>
            <td><%= job.start_date %></td>
            <td><%= job.days_to_complete %></td>
            <td><%= job.complete %></td>
            <td>
                <% s = "" %>
                <% job.employees.each do |employee| %>
                    <% s += employee.first_name + ", " %>
                <% end %>
                <%= s %>
            </td>
        </tr>
    <% end %>
</table>